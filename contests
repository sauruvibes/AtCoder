// [1] AtCoder Beginner Contest 363 

// [A]
#include<iostream>
using namespace std;
int main(){
  int n;
  cin>>n;
  if(n<100){
    cout<<(100-n);
  }else if(n<200){
    cout<<(200-n);
  }else if(n<300){
      cout<<(300-n);
  }else{
      cout<<(400-n);
  }
  return 0;
} 


//Toyota Programming Contest 2024#8（AtCoder Beginner Contest 365）
// [A]
#include<iostream>
using namespace std;
int main(){
    int y;
    cin>>y;
    
    if(y % 4 != 0){
        cout<<"365"<<endl;
    }else if(y % 4 == 0 && y % 100 != 0){
        cout<<"366"<<endl;
    }else if(y % 100 == 0 && y % 400 != 0){
        cout<<"365"<<endl;
    }else if(y % 400 == 0){
        cout<<"366"<<endl;
    }
    return 0;
}
